<h2>Коэффициенты премии</h2>
<table class="table table-striped table-sm">
	<thead>
		<tr>
			<th>От % (включительно)</th>
			<th>До % (включительно)</th>
			<th>Повышающий коэффициент</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @factors.each do |factor| %>
			<tr>
				<td><%= factor.min %></td>
				<td><%= factor.max %></td>
				<td><%= factor.value/100 %></td>
				<td></td>
			</tr>
		<% end %>
	</tbody>
</table>